{
  "name": "hackathon-root",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "tsc --project tsconfig.node.json",
    "start:server": "node dist/server/src/server/src/index.js",
    "start:client": "vite",
    "start": "concurrently \"npm run start:client\" \"npm run build:server && npm run start:server\"",
    "typeorm:migrate": "npx typeorm migration:run --dataSource dist/server/core/db.js"
  },
